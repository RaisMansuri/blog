{
  "name": "realtime",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "postinstall": "npm run build:ssr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "ng build --prod",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/realtime/stats.json",
    "dev:ssr": "ng run realtime:serve-ssr",
    "serve:ssr": "node dist/realtime/server/main.js",
    "build:ssr": "ng build --prod --aot --vendor-chunk --common-chunk  --buildOptimizer && ng run realtime:server:production",
    "prerender": "ng run realtime:prerender",
    "start:heroku": "node dist/realtime/server"
  },
  "private": true,
